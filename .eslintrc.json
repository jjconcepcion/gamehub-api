{
    "env": {
        "es6": true,
        "node": true,
        "jest": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": 2017
    },
    "rules": {
        "semi": [
            "error",
            "always"
        ],
        "no-underscore-dangle": [
            "error", { "allow": ["_id"] }
        ]
    }
}

